# Database Configuration
DATABASE_URL="mysql://user:password@localhost:3306/cawi_db"

# JWT Configuration
JWT_SECRET_ADMIN="your-super-secret-key-change-this-in-production"
JWT_ADMIN_EXPIRES_IN="3600"

# Fallback Secret (jika JWT_SECRET_ADMIN tidak ada)
SECRET_KEY="your-fallback-secret-key"

# Server Configuration
PORT=3000
NODE_ENV=development

# Mailer Configuration (jika diperlukan)
MAILER_USER="your-email@gmail.com"
MAILER_PASSWORD="your-app-password"
MAILER_FROM="noreply@cawi.com"
